<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
    <name>TrekTV</name>
    <match>all</match>
    <rule field="tvshow" operator="startswith">
        <value>Star Trek</value>
    </rule>
    <limit>150</limit>
    <order direction="ascending">random</order>
</smartplaylist>
